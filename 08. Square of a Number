using System;
class SquareOf12345
{
    static void Main()
    {
        double square = Math.Pow(12345,2);
        System.Console.WriteLine("This console application  calculates and prints the square of the number 12345");
        System.Console.WriteLine();
        System.Console.WriteLine();
        System.Console.WriteLine("the result is");
        System.Console.WriteLine();
        System.Console.WriteLine();
        System.Console.WriteLine(square);
        System.Console.WriteLine();
        System.Console.WriteLine();
    }
}
